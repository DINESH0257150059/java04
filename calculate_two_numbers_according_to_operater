import java.util.Scanner;

class HelloWorld {
    
    static int a;
    static char in;
    static int b;
    
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
        a=sc.nextInt();
        in=sc.next().charAt(0);
        b=sc.nextInt();
        
        switch(in){
            case '+':System.out.print(a+b);
            break;
            case '-':System.out.print(a-b);
            break;
            case '*':System.out.print(a*b);
            break;
            case '/':System.out.print(a/b);
            break;
            default:System.out.print("enter valid input");
        }
    }
}
