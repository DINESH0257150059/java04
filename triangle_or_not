import java.util.Scanner;
class HelloWorld {
    static int a;
    static int b;
    static int c;
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        if((a+b)>c && b+c>a && c+a>b)
        {
            System.out.println("Form a Triangle");
        }
        else
            System.out.println("It does not form a Triangle");
        
    }
}
